<h1>Form is here.</h1>
	<%= form_for @user, :url => users_path, :html => { :multipart => true } do |f| %>   
		<%=f.label :email %>
		<%=f.text_field :email %>
		<%= f.file_field :avatar %> 
		<%=f.label :password %>
		<%=f.text_field :password %>
		<%=f.label :password_confirmation %>
		<%=f.text_field :password_confirmation %>
		<%=f.submit %>
	<% end %>


